<h1>Listing review_owners</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Owner</th>
      <th>Review</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @review_owners.each do |review_owner| %>
      <tr>
        <td><%= review_owner.user_id %></td>
        <td><%= review_owner.owner_id %></td>
        <td><%= review_owner.review %></td>
        <td><%= review_owner.rating %></td>
        <td><%= link_to 'Show', review_owner %></td>
        <td><%= link_to 'Edit', edit_review_owner_path(review_owner) %></td>
        <td><%= link_to 'Destroy', review_owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Review owner', new_review_owner_path %>
